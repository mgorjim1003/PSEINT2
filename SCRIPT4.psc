Algoritmo SCRIPT4
	Escribir "Indicame un número: "
	Leer num1
	Escribir "Indicame otro número: "
	Leer num2
	Si num1 > num2 Entonces
		Escribir "El " num1 " es mayor"
	SiNo
		Si num2 > num1 Entonces
			Escribir "El " num2 " es mayor"
		Sino
			Escribir "Los números son iguales, coinciden."
		FinSi
	FinSi
FinAlgoritmo
